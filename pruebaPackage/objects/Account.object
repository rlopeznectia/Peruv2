    <fields>
        <fullName>SPI_Segment__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
				AND(
								NumberOfEmployees &lt; 50),
				&quot;Small&quot;, IF(
								AND(
								    NumberOfEmployees &gt;= 50,
								    NumberOfEmployees &lt; 200),
								&quot;Medium&quot;, IF(
												AND(
											     NumberOfEmployees &gt;= 200),
												&quot;Large&quot;, IF(
																ISBLANK(NumberOfEmployees),
																&quot;N/A&quot;, NULL)
												)
								)
				)</formula>
        <label>SPI Segment</label>
