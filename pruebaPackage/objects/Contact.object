        <fullName>CO005_LargoTelefonoMovil</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(MobilePhone)),
OR(
LEN(MobilePhone) &gt; 12,
LEN(MobilePhone) &lt; 10)
)</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>CO005 Debe ingresar el largo de teléfono correcto  en formato internacional +(código de país)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CO006_LargoTelefono</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(Phone)),
OR(
LEN(Phone) &gt; 12,
LEN(Phone) &lt; 10)
)</errorConditionFormula>
