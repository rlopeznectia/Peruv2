    <validationRules>
        <fullName>OPP001_MotivoPerdida</fullName>
        <active>true</active>
        <description>OPP001_Motivo Pérdida Obligatorio, Valida si la oportunidad pasa a etapa Perdida entonces deberá diligenciar el motivo de perdida</description>
        <errorConditionFormula>AND(
TEXT(StageName)=&quot;Cerrada Pérdida&quot;,
ISBLANK(TEXT(Loss_Reason__c))
)</errorConditionFormula>
        <errorDisplayField>Loss_Reason__c</errorDisplayField>
        <errorMessage>OPP001: Debe diligenciar el motivo de perdida de la oportunidad</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP004_CerradaGanada</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;5.Closed won&quot;),
TEXT(SBQQ__PrimaryQuote__r.SBQQ__Status__c) &lt;&gt; &quot;Rejected&quot;
)</errorConditionFormula>
        <errorDisplayField>SBQQ__PrimaryQuote__c</errorDisplayField>
        <errorMessage>OPP004: La Oportunidad  &quot;Cerrada Ganada&quot; debe tener un primary Quote Aceptado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP005_ValidaCreacionNuevo</fullName>
        <active>true</active>
        <description>Valida la creación de Oportunidades en Etapa Nuevo.</description>
        <errorConditionFormula>ISNEW() &amp;&amp;
NOT(ISPICKVAL(StageName, &quot;1.Nuevo&quot;))</errorConditionFormula>
        <errorMessage>OPP005:No puedes crear una Oportunidad en una etapa diferente a Nuevo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP006M_NombreComercialObligatorio</fullName>
        <active>false</active>
        <description>Valida que el campo nombre comercial este diligenciado cuando pase a la etapa Aceptación</description>
        <errorConditionFormula>AND(
TEXT(StageName)=&apos;4.Oferta Aceptada&apos;,
ISBLANK((SDX_NombreComercial__c)),
RecordType.Name = &apos;Merchant&apos;
)</errorConditionFormula>
        <errorDisplayField>SDX_NombreComercial__c</errorDisplayField>
        <errorMessage>OPP006: Debe diligenciar el nombre comercial o nombres comerciales</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP007M_ValidacionNegociacionSoloDescub</fullName>
        <active>true</active>
        <description>Validará que solo se pueda pasar a la etapa Negociación si es que el anterior estado fue Negociación.</description>
        <errorConditionFormula>AND(
				ISCHANGED(StageName),
    TEXT(PRIORVALUE(StageName)) &lt;&gt; &apos;2.Descubrimiento&apos;,
    TEXT(StageName)= &apos;3.Negociación&apos;,
				$RecordType.Name=&apos;Merchant&apos;,
    NOT(
        OR(
            $Profile.Name= &apos;Marketing&apos;
        
								)
    )
)</errorConditionFormula>
        <errorMessage>OPP007 : A la etapa &apos;Negociación&apos; podrá ser actualizada manualmente solo si la etapa anterior es &apos;Descubrimiento&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP007_ValidacionNegociacionSoloDescub</fullName>
        <active>true</active>
        <description>Validará que solo se pueda pasar a la etapa Negociación si es que el anterior estado fue Negociación.</description>
        <errorConditionFormula>AND(
				ISCHANGED(StageName),
    TEXT(PRIORVALUE(StageName)) &lt;&gt; &apos;2.Descubrimiento&apos;,
    TEXT(StageName)= &apos;3.Negociación&apos;,
    NOT(
        OR(
            $Profile.Name= &apos;Marketing&apos;
        
			 				)
    )
)</errorConditionFormula>
        <errorMessage>OPP007 : A la etapa &apos;Negociación&apos; podrá ser actualizada manualmente solo si la etapa anterior es &apos;Descubrimiento&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP008M_SoloAceptacionDesdeNegociacion</fullName>
        <active>true</active>
        <description>Validará que solo se pueda pasar a la etapa Aceptación si es que el anterior estado fue Negociación.</description>
        <errorConditionFormula>AND(
ISCHANGED(StageName),
TEXT(PRIORVALUE(StageName)) &lt;&gt; &apos;3.Negociación&apos;,
TEXT(StageName)= &apos;4.Oferta Aceptada&apos;,
$RecordType.Name = &apos;Merchant&apos;,
NOT(
OR(
$Profile.Name= &apos;Marketing&apos;
)
)
)</errorConditionFormula>
        <errorMessage>OPP008 : A la etapa &apos;4.Oferta Aceptada&apos; podrá ser actualizada manualmente solo si la etapa anterior es &apos;3.Negociación&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP008_SoloAceptacionDesdeNegociacion</fullName>
        <active>true</active>
        <description>Validará que solo se pueda pasar a la etapa Aceptación si es que el anterior estado fue Negociación.</description>
        <errorConditionFormula>AND(
ISCHANGED(StageName),
TEXT(PRIORVALUE(StageName)) &lt;&gt; &apos;3.Negociación&apos;,
TEXT(StageName)= &apos;4.Oferta Aceptada&apos;,
NOT(
OR(
$Profile.Name= &apos;Marketing&apos;
)
)
)</errorConditionFormula>
        <errorMessage>OPP008 : A la etapa &apos;4.Oferta Aceptada&apos; podrá ser actualizada manualmente solo si la etapa anterior es &apos;3.Negociación&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP009_GanadaSoloAceptacion</fullName>
        <active>true</active>
        <description>Validará que solo se pueda pasar a la etapa Ganada si es que el anterior estado fue Aceptación.</description>
        <errorConditionFormula>AND(
ISCHANGED(StageName),
TEXT(PRIORVALUE(StageName)) &lt;&gt; &apos;4.Oferta Aceptada&apos;,
TEXT(StageName)= &apos;5.Closed won&apos;,
NOT(
OR(
$Profile.Name= &apos;Marketing&apos;
)
)
)</errorConditionFormula>
        <errorMessage>OPP009 : A la etapa &apos;5.-Cerrada Ganada&apos; podrá ser actualizada manualmente solo si la etapa anterior es &apos;4.Oferta Aceptada&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP010_GanadaNoModificacion</fullName>
        <active>false</active>
        <description>Validación para que una vez ganada la oportunidad ya no se modifique.</description>
        <errorConditionFormula>AND(
TEXT(PRIORVALUE(StageName))=&apos;5.Closed won&apos;,
NOT(
OR(

$Profile.Name= &apos;Marketing&apos;,
$Profile.Name= &apos;Administrador&apos;

)
)
				
)</errorConditionFormula>
        <errorMessage>OPP010 : No se pueden realizar cambios cuando una oportunidad está en estado Cerrada Ganada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP011_ValidacionEtapasConsecutivas</fullName>
        <active>true</active>
        <description>Validación solo para que el salto de etapas solo sea consecutivo</description>
        <errorConditionFormula>AND(
ISCHANGED(StageName),
OR(
AND(
TEXT(PRIORVALUE(StageName))=&apos;1.Nuevo&apos;,
TEXT(StageName) = &apos;3.Negociación&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;1.Nuevo&apos;,
TEXT(StageName) = &apos;4.Oferta Aceptada&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;1.Nuevo&apos;,
TEXT(StageName) = &apos;5.Closed won&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;2.Descubrimiento&apos;,
TEXT(StageName) = &apos;4.Oferta Aceptada&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;2.Descubrimiento&apos;,
TEXT(StageName) = &apos;5.Closed won&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;3.Negociación&apos;,
TEXT(StageName) = &apos;5.Closed won&apos;
)
),
NOT(
OR(

$Profile.Name= &apos;Marketing&apos;
)
)
)</errorConditionFormula>
        <errorMessage>OPP011 : Sólo se puede pasar de una etapa a otra consecutiva.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP012_ValidacionEtapaNoRetroceso</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(StageName),
OR(
TEXT(StageName) = &apos;1.Nuevo&apos;,
AND(
TEXT(PRIORVALUE(StageName))=&apos;3.Negociación&apos;,
TEXT(StageName) = &apos;2.Descubrimiento&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;4.Oferta Aceptada&apos;,
TEXT(StageName) = &apos;3.Negociación&apos;
),
AND(
TEXT(PRIORVALUE(StageName))=&apos;4.Oferta Aceptada&apos;,
TEXT(StageName) = &apos;2.Descubrimiento&apos;
),

TEXT(PRIORVALUE(StageName))=&apos;6.Cerrada Pérdida&apos;

),
NOT(
OR(

$Profile.Name= &apos;Marketing&apos;
)
)
)</errorConditionFormula>
        <errorMessage>OPP012 : No puedes retroceder etapas.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP013_ValidacionInterlocutor</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(StageName,&quot;2.Descubrimiento&quot;) &amp;&amp; 
ISBLANK(Interlocutor__c)</errorConditionFormula>
        <errorMessage>OPP013: Para avanzar a la etapa Descubrimiento debes completar el Interlocutor.</errorMessage>
    </validationRules>
</CustomObject>
